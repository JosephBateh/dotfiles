# Git Aliases
alias gs='git status'
alias ga='git add .'
alias gac='git add . && git commit'
alias gac-amend='git add . && git commit --amend'

# Directory Aliases
alias dot-ws='cd ~/.dotfiles'
alias root-ws='cd ~/root'
alias code-ws='cd ~/root/code'
alias go-ws='cd ~/root/code/go/src/github.com/josephbateh'